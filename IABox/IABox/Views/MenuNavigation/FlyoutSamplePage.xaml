<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IABox.Views.MenuNavigation.FlyoutSamplePage"
             xmlns:local="clr-namespace:IABox.Views.MenuNavigation"
             Title="FlyoutSamplePage" Background="#ffffff">
    
        <FlyoutPage.Flyout>
            <local:FlyoutMenuPage x:Name="flyoutPage" />
        </FlyoutPage.Flyout>
        <FlyoutPage.Detail>
            <NavigationPage>
                <x:Arguments>
                    <local:FlyoutDetailsPage />
                </x:Arguments>
            </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>