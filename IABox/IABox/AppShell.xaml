<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="IABox.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:IABox"
    Shell.FlyoutBehavior="Disabled">

    <FlyoutItem>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Login}"
        Route="Login" />
        
        <Tab Route="FlyoutPage">
            <ShellContent Route="FlyoutPage" Title="FlyoutPage"/>
            <ShellContent Route="Inicio" Title="Inicio"/>
            <ShellContent Route="Alarmas" Title="Alarmas"/>
            <ShellContent Route="Configuraciones" Title="Configuraciones"/>
        </Tab>
    </FlyoutItem>
  
     
</Shell>
